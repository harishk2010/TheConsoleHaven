                    </li>
                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Edit Categories </h2>
                    <p>Edit category</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Categories" class="form-control bg-white">
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    {{#if catSaveMsg}}
            <div class="alert alert-success mt-3 alert-dismissible fade  {{#if catSaveMsg}}show{{/if}}" role="alert">
              {{catSaveMsg}}
              <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            {{/if}}
            {{#if catExistMsg}}
            <div class="alert alert-danger mt-3 alert-dismissible fade {{#if catExistMsg}}show{{/if}}" role="alert">
              {{catExistMsg}}
              <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            {{/if}}
                    <div class="row">
                        <div class="col-md-12">
                            <form action="/admin/editCategory/{{cat._id}}" method="post" class="" enctype="multipart/form-data">
                                <div class="mb-4">
                                    <label for="categoryName" class="form-label">Category Name:</label>
                                    <input type="text"  class="form-control" name="name" id="product_name" value="{{cat.category}}" required />
                                </div>
                                
                                
                                <div class="mb-4">
                                    <label for="categoryImage" class="form-label">Category Image:</label>
                                    <input type="file" placeholder="{{cat._id}}" class="form-control-file" src="/images/products/{{image.[0]}}" name="image" id="product_name"  />
                                    
                                    
                                </div>
                                <div class="d-flex g-2">
                                    <div class="m-2">
                                         <button type="submit" class="btn btn-primary">Edit category</button>

                                    </div>
                                    <div class="m-2">
                                        <a href="/admin/category" type="button" class="btn btn-primary">back</a>

                                    </div>
                                   
                                    
                                </div>
                            </form>
                        </div>
                     
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->
            </div> <!-- card .// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
